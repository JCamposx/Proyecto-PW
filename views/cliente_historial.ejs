<html>
    <head>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">  
    </head>
    <body class="container">
        <div>
            Historial
        </div>
        <table class="table">
            <thead>
              <tr>
                <th scope="col">Id</th>
                <th scope="col">Fecha</th>
                <th scope="col">Hora</th>
                <th scope="col">NÃºmero</th>
                <th scope="col">Equipo A</th>
                <th scope="col">Equipo B</th>
                <th scope="col">Categoria</th>
                <th scope="col">Juego</th>
                <th scope="col">Equipo Apostado</th>
                <th scope="col">Estado</th>
                <th scope="col">Monto Apostado</th>
                <th scope="col">Factor</th>
                <th scope="col">Resultado</th>
              </tr>
            </thead>
            <tbody>
              <% partidas.forEach(function(partidas){%>
              <tr>
                <th scope="row"><%= partidas.id %></th>
                <td><%=partidas.fecha %></td>
                <td><%=partidas.hora %></td>
                <td><%=partidas.numero %></td>
                <td><%=partidas.equipoa %></td>
                <td><%=partidas.equipob %></td>
                <td><%=partidas.categoria %></td>
                <td><%=partidas.juego %></td>
                <td><%=partidas.equipoapostado %></td>
                <td><%=partidas.estado %></td>
                <td><%=partidas.monto %></td>
                <td><%=partidas.factor %></td>
                <td><%= (partidas.monto)*(partidas.factor) %></td>
              </tr>
              <%  });%>
            </tbody>
          </table>
    </body>
</html>